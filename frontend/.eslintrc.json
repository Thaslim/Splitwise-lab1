{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-quotes": ["error", "prefer-single"],
        "react/jsx-one-expression-per-line": [0,{ "allow": "none" }],
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "always-multiline",
            "imports": "always-multiline",
            "exports": "never",
            "functions": "never"
        }],
        "no-unused-vars":"warn"
      }
}
